{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "cropperjs@1.5.8",
      "destination": "wwwroot/lib/cropperjs/"
    },
    {
      "provider": "unpkg",
      "library": "chart.js@2.9.3",
      "destination": "wwwroot/lib/chart.js/",
